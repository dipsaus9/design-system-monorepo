{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "outputs": ["build/**", "!.cache/**", "dist/**"],
      "dependsOn": ["^build"]
    },
    "lint": {},
    "lint:fix": {},
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "test:ci": {},
    "test:e2e:ci": {},
    "typecheck": {
      "dependsOn": ["^build"]
    }
  }
}
