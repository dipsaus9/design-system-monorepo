  },
  "devDependencies": {
    "@dipsaus9/eslint-config": "workspace:*",
    "@dipsaus9/tsconfig": "workspace:*",
    "@storybook/react": "^7.4.3",
    "@turbo/gen": "^1.10.12",
    "@types/node": "^20.5.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.30",
    "tailwindcss": "^3.3.3",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@dipsaus9/tailwind-theme": "workspace:*",
    "class-variance-authority": "^0.7.0",
    "react": "^18.2.0"
  },
  "exports": {
    ".": "./src/index",
    "./src/*": "./src/*"
